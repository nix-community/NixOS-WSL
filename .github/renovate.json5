{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'config:recommended',
    ':semanticCommitTypeAll(chore)',
    'helpers:pinGitHubActionDigests',
  ],
  labels: [
    'release-exclude',
    'renovate',
  ],
  baseBranchPatterns: [
    'main',
    '/^release-.+$/',
    'renovate-test',
  ],
  nix: {
    enabled: true,
  },
  lockFileMaintenance: {
    enabled: true,
    extends: [
      'schedule:weekly',
    ],
    automerge: true,
  },
  customManagers: [
    {
      customType: 'regex',
      managerFilePatterns: [
        '/.github\\/.+\\.ya?ml$/',
      ],
      matchStrings: [
        'repository:\\s*(?<depName>[^/\\s]+/[^/\\s]+)\\s*ref:\\s*(?<currentValue>[^\\s]+)',
      ],
      datasourceTemplate: 'github-tags',
    },
  ],
  packageRules: [
    {
      matchBaseBranches: [
        '/^release-.+$/',
      ],
      prPriority: -1,
    },
    {
      matchBaseBranches: [
        '/^release-.+$/',
      ],
      matchManagers: [
        'nix',
      ],
      matchUpdateTypes: [
        'major',
        'minor',
        'patch',
      ],
      enabled: false,
    },
    {
      matchBaseBranches: [
        '/^release-.+$/',
      ],
      matchManagers: [
        'cargo',
      ],
      enabled: false,
    },
    {
      matchManagers: [
        'github-actions',
      ],
      matchPackageNames: [
        '/^actions//',
      ],
      matchUpdateTypes: [
        'minor',
        'patch',
      ],
      automerge: true,
    },
    {
      matchBaseBranches: [
        'main',
      ],
      matchUpdateTypes: [
        'pin',
        'pinDigest',
      ],
      groupName: '{{manager}} pin versions',
      automerge: true,
    },
    {
      matchUpdateTypes: [
        'lockFileMaintenance',
      ],
      groupName: '{{manager}} lock file maintenance',
    },
  ],
}
